    "import/order": [
      "warn",
      {
        "groups": [
          "builtin",
          "external"
        ],
        "pathGroups": [
          {
            "pattern": "@epconnect/**",
            "group": "external",
            "position": "after"
          }
        ],
        "newlines-between": "always-and-inside-groups",
        "distinctGroup": true,
        "named": false,
        "warnOnUnassignedImports": false
      }
    ],
    "no-console": [
