      "warn"
    ],
    "@typescript-eslint/no-unused-vars": [
      "warn",
      {
        "argsIgnorePattern": "^_",
        "varsIgnorePattern": "^_"
      }
    ],
    "sort-imports": [
      "warn",
      {
        "allowSeparatedGroups": true,
        "memberSyntaxSortOrder": [
          "none",
          "all",
          "single",
          "multiple"
        ],
        "ignoreCase": false,
        "ignoreDeclarationSort": false,
        "ignoreMemberSort": false
      }
    ],
